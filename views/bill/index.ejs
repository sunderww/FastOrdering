<body class="dashboard-page sb-l-o sb-r-c">
    
    <!-- Start: Main -->
    <div id="main">
        
        <% include ../header.ejs %>
        <% include ../sidebar.ejs %>
        
        <!-- Start: Content-Wrapper -->
        <section id="content_wrapper">
            
            
            
            <!-- Start: Topbar -->
            <header id="topbar">
                <div class="topbar-left">
                    <ol class="breadcrumb">
                        <li class="crumb-active">
                            <a href="">Options</a>
                        </li>
                        <li class="crumb-icon">
                            <a href="/dashboard">
                                <span class="glyphicon glyphicon-home"></span>
                            </a>
                        </li>
                        <li class="crumb-link">
                            <a href="dashboard">Accueil</a>
                        </li>
                        <li class="crumb-trail">Facturation</li>
                    </ol>
                </div>
                
            </header>
            <!-- End: Topbar -->
            
            
            
            <!-- Begin: Content -->
            <section id="content" class="animated fadeIn">
                <!-- Dashboard Tiles -->
                
                <div class="col-md-12">
                    <div class="panel">
                        <div class="panel-heading">
                            <span class="panel-title">Factures :</span>
                        </div>
                        <table class="table table-striped">
                            <tr>
                                <td>Table :</td>
                                <td>Nb personnes :</td>
                                <td>Heure cr√©ation :</td>
                                <td></td>
                                <td></td>
                            </tr>
                            <% _.each(orders, function(order) { %>
                            <tr data-id="<%= order.id %>" data-model="order">
                                <td><%= order.table_id %></td>
                                <td><%= order.dinerNumber %></td>
                                <td><%= order.time %></td>
                                <td><a href="/user/edit/<%= order.id %>" class="btn btn-sm btn-warning">Imprimer</a></td>
                                <td><a href="/user/destroy/<%= order.id %>" class="btn btn-sm btn-danger">Editer</a></td> 
                                <td></td>
                            </tr>
                            <% }); %>
                        </table>
                    </div>
                </div>
            </section>
                                
<!-- End: Main -->

